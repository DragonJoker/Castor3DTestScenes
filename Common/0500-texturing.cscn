materials phong

scene "Scene"
{
	ambient_light 0.2 0.2 0.2

	skybox
	{
		equirectangular "Textures/backgroundSkyboxEquirectangular.jpg" 1024
	}

	sampler "Linear"
	{
		min_filter linear
		mag_filter linear
		min_lod 0.0
		max_lod 0.0
		lod_bias 0.0
		u_wrap_mode clamp_to_edge
		v_wrap_mode clamp_to_edge
		w_wrap_mode clamp_to_edge
		border_colour float_opaque_black
	}

	mesh "Plane"
	{
		type "plane" -width_subdiv=1 -depth_subdiv=1 -width=10.0 depth=10.0
	}

	material "DiffuseMap"
	{
		pass
		{
			diffuse 0.75164 0.75164 0.75164 1.0
			emissive 0.0 0.0 0.0 1.0
			shininess 50.2
			two_sided true

			texture_unit
			{
				image "Textures/Diffuse.jpg"
				channel diffuse
				sampler	"Linear"
			}
		}
	}

	scene_node "Diffuse"
	{
		position -10.0 -5.0 0.0
	}
	object "Diffuse"
	{
		parent "Diffuse"
		mesh "Plane"
		material "DiffuseMap"
	}

	material "SpecularMap"
	{
		pass
		{
			diffuse 0.75164 0.75164 0.75164 1.0
			emissive 0.0 0.0 0.0 1.0
