				image "Textures/Diffuse.jpg"
				channel diffuse
				sampler	"Linear"
			}

			texture_unit
			{
				image "Textures/Specular.jpg"
				channel specular
				sampler	"Linear"
			}
		}
	}

	scene_node "Specular"
	{
		position 0.0 -5.0 0.0
	}
	object "Specular"
