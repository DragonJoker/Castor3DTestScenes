			{
				type int
				name linear_search_steps
				value 100
			}

			variable
			{
				type int
				name binary_search_steps
				value 100
			}

			variable
			{
				type float
				name depth
				value 0.075
			}

			variable
			{
				type float
				name tile
				value 1.0
			}
		}
	}
}

