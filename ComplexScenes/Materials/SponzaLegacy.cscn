
// Samplers

sampler "Trilinear"
{
	min_filter linear
	mag_filter linear
	mip_filter linear
	u_wrap_mode repeat
	v_wrap_mode repeat
	w_wrap_mode repeat
	min_lod 0
	max_lod 5
	lod_bias 0
	max_anisotropy 1
}

// Materials

material "Material__25"
{
	pass
	{
		diffuse 0.588000 0.588000 0.588000
		specular 0.000000 0.000000 0.000000
		ambient 1.0
		shininess 64
		two_sided true

		texture_unit
		{
			sampler "Trilinear"
			levels_count 11
			channel diffuse
			image "Textures/SponzaLegacy/lion.png" rgb
		}

		texture_unit
		{
			sampler "Trilinear"
			levels_count 11
			channel normal
			image "Textures/SponzaLegacy/Lion_Normal.png" rgb
		}
	}
}

